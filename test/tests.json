[
    {
        "title": "Adding an Object Member",
        "input": { "foo": "bar"},
        "patch": [{ "op": "add", "path": "/baz", "value": "qux" }],
        "output": {"baz": "qux", "foo": "bar"}
    },
    {
        "title": "Adding an Array Element",
        "input": { "foo": [ "bar", "baz" ] },
        "patch": [{ "op": "add", "path": "/foo/1", "value": "qux" }],
        "output": { "foo": [ "bar", "qux", "baz" ] } 
    },
    {
        "title": "Removing an Object Member",
        "input": { "baz": "qux", "foo": "bar" },
        "patch": [ { "op": "remove", "path": "/baz" } ],
        "output": { "foo": "bar" }
    },
    {
        "title": "Removing an Array Element",
        "input": { "foo": [ "bar", "qux", "baz" ] },
        "patch": [ { "op": "remove", "path": "/foo/1" } ],
        "output": { "foo": [ "bar", "baz" ] }
    },
    {
        "title": "Replacing a Value",
        "input": { "baz": "qux", "foo": "bar" },
        "patch": [ { "op": "replace", "path": "/baz", "value": "boo" } ],
        "output": { "baz": "boo", "foo": "bar" }

    },
    {
        "title": "Moving a Value",
        "input": { "foo": { "bar": "baz", "waldo": "fred" }, "qux": { "corge": "grault" } } ,
        "patch": [ { "op": "move", "from": "/foo/waldo", "path": "/qux/thud" } ],
        "output": { "foo": { "bar": "baz" }, "qux": { "corge": "grault", "thud": "fred" } }
    },
    {
        "title": "Moving an Array Element",
        "input": { "foo": [ "all", "grass", "cows", "eat" ] },
        "patch": [ { "op": "move", "from": "/foo/1", "path": "/foo/3" } ],
        "output": { "foo": [ "all", "cows", "eat", "grass" ] }
    },
    {
        "title": "Testing a Value: Success",
        "input": { "baz": "qux", "foo": [ "a", 2, "c" ] } ,
        "patch": [ { "op": "test", "path": "/baz", "value": "qux" },
            { "op": "test", "path": "/foo/1", "value": 2 } ],
        "output": { "baz": "qux", "foo": [ "a", 2, "c" ] }
    },
    {
        "title": "Testing a Value: Error",
        "input": { "baz": "qux" },
        "patch": [ { "op": "test", "path": "/baz", "value": "bar" } ],
        "error": true
    }, 
    {
        "title": "Adding a Nested Member Object",
        "input": { "foo": "bar" },
        "patch": [ { "op": "add", "path": "/child", "value": { "grandchild": { } } } ],
        "output": { "foo": "bar", "child": { "grandchild": { } } }
    }, 
    {
        "title": "Ignoring Unrecognized Elements",
        "input": { "foo": "bar" },
        "patch": [ { "op": "add", "path": "/baz", "value": "qux", "xyz": 123 } ],
        "output": { "foo": "bar", "baz": "qux" }
    }, 
    {
        "title": "Adding to a Nonexistent Target",
        "input": { "foo": "bar" },
        "patch": [ { "op": "add", "path": "/baz/bat", "value": "qux" } ],
        "error": true
    }, 
    {
        "title": "Adding an Array Value",
        "input": { "foo": ["bar"] },
        "patch": [ { "op": "add", "path": "/foo/-", "value": ["abc", "def"] } ],
        "output": { "foo": ["bar", ["abc", "def"]] }
    }
]
